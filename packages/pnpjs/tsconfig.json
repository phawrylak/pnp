        "../sp-addinhelpers/index.ts",
        "../sp-addinhelpers/src/**/*.ts"
    ],
    "references": [
        {
            "path": "../common"
        },
        {
            "path": "../logging"
        },
        {
            "path": "../odata"
        },
        {
            "path": "../sp"
        },
        {
            "path": "../graph"
        },
        {
