    "include": [
        "./index.ts",
        "./src/**/*.ts",
        "../common/index.ts",
        "../common/src/**/*.ts",
        "../logging/index.ts",
        "../logging/src/**/*.ts",
        "../odata/index.ts",
        "../odata/src/**/*.ts",
